{
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)",
    "packages:eslint"
  ],
  "ignorePresets": [
    ":prHourlyLimit2"
  ],
  "packageRules": [
    {
      "updateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ]
}
