{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "tempDirName": "stryker-tmp",
  "testRunner": "jest",
  "jest": {
    "configFile": "jest.config.json"
  },
  "coverageAnalysis": "perTest",
  "tsconfigFile": "tsconfig.json",
  "checkers": [
    "typescript"
  ],
  "thresholds": {
    "break": 100
  },
  "ignorePatterns": [
    "lib",
    "*.e2e-spec.ts"
  ],
  "timeoutFactor": 5,
  "timeoutMS": 60000
}
