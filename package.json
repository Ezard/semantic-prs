{
  "name": "semantic-prs",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub app to check that pull request titles follow the Conventional Commits spec",
  "author": "Ben Ezard <ezard.ben@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/Ezard/semantic-prs",
  "keywords": [
    "github",
    "pr",
    "pull-request",
    "conventional-commits"
  ],
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint src",
    "lint:style": "prettier --check src",
    "test": "jest"
  },
  "dependencies": {
    "@conventional-commits/parser": "0.4.1",
    "conventional-commit-types": "3.0.0",
    "probot": "12.2.2"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "5.6.0",
    "@types/jest": "27.5.0",
    "@types/node": "16.11.33",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.0",
    "nock": "13.2.4",
    "prettier": "2.6.2",
    "smee-client": "1.2.3",
    "ts-jest": "28.0.1",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": ">= 16"
  }
}
